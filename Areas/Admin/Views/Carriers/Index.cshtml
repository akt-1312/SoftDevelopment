@model IEnumerable<Hospital.Models.Carriers>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cr_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MOB)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Salary)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cr_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MOB)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Salary)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Cr_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Cr_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Cr_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
